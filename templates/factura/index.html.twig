{% extends 'base.html.twig' %}

{% block title %}Factura index{% endblock %}
{% block body %}

<input class="form-control" type="text" value="Cuadro de texto...">

  
    <h1>Factura index</h1>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
        {% for factura in facturas %}
            <tr>
                <td>{{ factura.id }}</td>
                <td>
                    <a href="{{ path('factura_show', {'id': factura.id}) }}">show</a>
                    <a href="{{ path('factura_edit', {'id': factura.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
</table>
  

   

    <a href="{{ path('factura_new') }}">Create new</a>
{% endblock %}